{
    "name": "aftdev/log-manager",
    "description": "Log manager for monolog",
    "license": "BSD-3-Clause",
    "keywords": ["monolog"],
    "require": {
        "php": "^8.0",
        "aftdev/service-manager": "^1.1",
        "monolog/monolog": "^1.24.0"
    },
    "autoload": {
        "psr-4": {
            "AftDev\\Log\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AftDevTest\\Log\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "extra": {
        "laminas": {
            "config-provider": "AftDev\\Log\\ConfigProvider"
        }
    }
}
